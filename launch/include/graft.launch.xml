<launch>

  <node name="graft" pkg="graft" type="graft_ukf_velocity">
    <rosparam file="$(find robomagellan)/config/graft.yaml" command="load" />
  </node>

  <node name="imu_bias_remover" pkg="imu_pipeline" type="imu_bias_remover">
    <param name="use_odom" value="true"/>
    <remap from="imu" to="imu_raw"/>
  </node>

</launch>
